<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.szut.dqi12.cheftrainer.client.view.fxmlcontrollers.dialogcontrollers.ChangeFormationController">
	<columnConstraints>
		<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints fillHeight="false" maxHeight="30.0"
			minHeight="10.0" percentHeight="0.0" vgrow="NEVER" />
		<RowConstraints minHeight="10.0" vgrow="ALWAYS" />
		<RowConstraints maxHeight="25.0" minHeight="25.0"
			prefHeight="25.0" />
	</rowConstraints>
	<children>
		<TableView fx:id="playerTable" GridPane.hgrow="ALWAYS"
			GridPane.rowIndex="1" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
			<columns>
				<TableColumn fx:id="nameCol" prefWidth="-1.0" text="Spieler" />
				<TableColumn fx:id="pointsCol" prefWidth="-1.0" text="Punkte" />
				<TableColumn fx:id="worthCol" prefWidth="-1.0" text="Wert" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
	</children>
</GridPane>

